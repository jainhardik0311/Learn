class Vehicle:

    def __init__(self, name, max_speed, mileage):
        self.name = name
        self.max_speed = max_speed
        self.mileage = mileage

class Bus(Vehicle):
    pass
child = Bus("School Volvo","180","12")
print("Vehicle name:",child.name,"Speed:",child.max_speed,"Mileage:",child.mileage)
